<div>

<div> {{ section.settings.logo_text }} </div>

<div> {{ section.settings.subtitle }}</div>

<h1> {{ section.settings.heading }}</h1>

<p> {{ section.settings.description }}</p>

<a href="#">

    {{ section.settings.button_text }}

</a>

<div> {{ section.settings.bottom_text }}</div>

</div>


{%  schema %}

{
 "name": "Banner Custom",
 "settings": [
    {
      "type": "text",
       "id": "logo_text",
       "label": "Logo Text",
       "default": "TISSO VISION"
    },

        {
      "type": "text",
       "id": "subtitle",
       "label": "subtitle",
       "default": "Find ideal gift for your loved ones."
    },

        {
      "type": "text",
       "id": "heading",
       "label": "Main heading",
       "default": "The Gift Guide."
    }, 

        {
      "type": "textarea",
       "id": "description",
       "label": "Description",
       "default": "Discover meaningful gifts crafted with care."
    },

        {
      "type": "text",
       "id": "button_text",
       "label": "Button Text",
       "default": "SHOP NOW"
    },

    {
      "type": "text",
      "id": "bottom_text",
      "label": "Bottom Text",
      "default":"SUSTAINABLE, EATHICALLY MADE"
    }
]}

{% endschema %}